{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/order.schema.json", 
    "title": "Order",
    "description": "Order for warehouse goods.",
    "type": "object",
    "properties": {
      "productId": {"type": "integer",
                    "minimum": 1000},
      "name": {"type": "string"},
      "addr": {"type": "string"},
      "zip": {"type": "integer"}
    },
    "additionalProperties": false,
    "required": [
      "productId",
      "name",
      "addr",
      "zip"
    ]
}